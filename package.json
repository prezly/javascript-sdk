{
    "name": "@prezly/sdk",
    "version": "6.22.0",
    "description": "Prezly API SDK",
    "main": "dist/index.js",
    "files": [
        "dist/**"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/prezly/javascript-sdk"
    },
    "author": "Prezly Team <hello@prezly.com> (https://www.prezly.com)",
    "license": "MIT",
    "engines": {
        "node": ">= 8.16.0"
    },
    "dependencies": {
        "@prezly/progress-promise": "^1.0.0",
        "@prezly/uploads": "^0.2.1",
        "query-string": "^6.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/preset-env": "^7.7.1",
        "@types/node": "^12.12.9",
        "babel-jest": "^26.6.3",
        "jest": "^26.6.3",
        "jest-fetch-mock": "^3.0.3",
        "np": "^7.2.0",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "build": "tsc --project .",
        "watch": "tsc --watch --preserveWatchOutput --project .",
        "start": "npm run build --incremental --watch",
        "test": "npm run test:build && npm run test:unit",
        "test:unit": "jest",
        "test:build": "node dist/index.js && tsc --noEmit dist/index.d.ts",
        "snapshot": "jest --updateSnapshot",
        "prettier:check": "prettier --check './src/**/*.{ts,js}'",
        "prettier:fix": "prettier --write './src/**/*.{ts,js}'",
        "prerelease": "npm run clean && npm run build",
        "release": "np"
    },
    "np": {
        "yarn": false
    }
}
